<div id="dashboard-header">
<% if current_user %>
    <div id="logged-in-user" class="small-font">
        <div class="login-nav">
         &nbsp;
        <%= link_to("Log Out", destroy_user_session_path) %>
            | <%= link_to("My GetUp!", dashboard_path) %>
        <% if current_user.is_admin? || current_user.is_volunteer? %>
            | <%= link_to("Admin", admin_root_path) %>
        <% end %>
         &nbsp;
        </div>
        <div id="mygetup-login">
         You are logged in as <i><%= "#{current_user.first_name}" %></i> &#9658; &nbsp;
        </div>
    </div>
<% else %>
        <div id="logged-in-user" class="small-font">
          <div class="login-nav">
              &nbsp; <%= link_to("My GetUp!", dashboard_path) %> &nbsp;
            </div>
            <div id="mygetup-logout">
            Login to edit your personal information &#9658; &nbsp;
            </div>
        </div>
<% end %>
</div>